<?xml version="1.0" encoding="utf-8" ?>
<Parameters> 
   <TonometerPulseHeightVariationHigh>20</TonometerPulseHeightVariationHigh> 
   <TonometerPulseHeightVariationLow>10</TonometerPulseHeightVariationLow> 
   <CuffSignalQThreshold>70</CuffSignalQThreshold> 
   <NIBPRestInterval>5</NIBPRestInterval> 
   <StdEM4Timeout>50</StdEM4Timeout> 
   <ExtEM4Timeout>100</ExtEM4Timeout> 
   <EM4NumberOfRetriesAllowed>3</EM4NumberOfRetriesAllowed> 
   <ProcessSequenceNumber>
		<CommandResponse>N</CommandResponse>
		<StreamingPackets>N</StreamingPackets>
   </ProcessSequenceNumber>
   <IdleModeSleepTime>3000</IdleModeSleepTime>
   <StreamingTimeoutFlag>Y</StreamingTimeoutFlag>
   <ProcessStatusFlagParts>
		<StopButton>Y</StopButton>
		<PowerUpBit>Y</PowerUpBit>
		<AlarmFlag>Y</AlarmFlag>
		<CuffStatusFlags>Y</CuffStatusFlags>
		<TonometerFlags>N</TonometerFlags>
		<UnusedBits>Y</UnusedBits>
   </ProcessStatusFlagParts>
  <Gui>
    <WaitButtonDelay>5</WaitButtonDelay>
    <AutoPWAProgressbarDelay>5</AutoPWAProgressbarDelay>
	<SQLServerVersionNumber>10.50.1600.1</SQLServerVersionNumber>
  </Gui>
	<Crx>
		<Logging>
			<Enable>Y</Enable>
	   </Logging>
	</Crx>
  <PWV>
    <Cuff>
      <Inflation>
        <Timeout>180</Timeout>
      </Inflation>
      <Deflation>
        <Timeout>10</Timeout>
        <Rest>5</Rest>
      </Deflation>
      <Reinflation>
        <Rest>5</Rest>
      </Reinflation>     
    </Cuff>
  </PWV>
  <PWA>
    <Cuff>
      <Inflation>
        <Timeout>180</Timeout>
      </Inflation>
	   <Deflation>
        <Timeout>10</Timeout>
       <Rest>5</Rest>
      </Deflation>
      <Reinflation>
        <Rest>5</Rest>
      </Reinflation>
      <PatientReport>
        <RangeArraySize>
          <SP>5</SP>
          <DP>5</DP>
          <HR>5</HR>
        </RangeArraySize>
      </PatientReport>
    </Cuff>
  </PWA>  
  <NibpCuffDataInterval>200</NibpCuffDataInterval>
  <NibpDal>
	<Timeouts>
		<!-- All timeout values are in miliseconds --> 
		<DefaultOWaitTime>100</DefaultOWaitTime>
		<StartBP>
			<WatitForO>1000</WatitForO>
			<WatitForK>140000</WatitForK>
		</StartBP>
		<StartPediatricBpWaitForK>100000</StartPediatricBpWaitForK>
		<StartNeonateBpWaitForK>85000</StartNeonateBpWaitForK>
		<DalCuffPressureCommandWaitTime>100</DalCuffPressureCommandWaitTime>
		<DalNibpGetBpDataCommandWaitTime>200</DalNibpGetBpDataCommandWaitTime>
		<DalNibpIntialInflateCommandWaitTime>50</DalNibpIntialInflateCommandWaitTime>
		<DalNibpAbortBpCommandWaitTime>200</DalNibpAbortBpCommandWaitTime>
		<SendNibpConnectCommand>
			<!--WaitTimeBetweenConnectAndStartBP: The time to wait(in milliseconds) after NIBP_CONNECT command has been sent and the Start_BP
					command will be sent. Can be zero but usuall results in the command being NACKED. Default value 1000 ms-->
			<WaitTimeBetweenConnectAndStartBP>1000</WaitTimeBetweenConnectAndStartBP>
			<BusyWaitPeriod>1000</BusyWaitPeriod>
		</SendNibpConnectCommand>
		<GetCuffPressureCommandRetryInterval>200</GetCuffPressureCommandRetryInterval>
	</Timeouts>
	<!--Disable the cuffpressure measurement thread for NIBP. For debugging only-->
	<CuffPressureMeasurementThreadEnable>Y</CuffPressureMeasurementThreadEnable>
	<!-- Ignores an NIBP GetBP Data response with EC =-1 for testing purpose and converts it to 0 so that the data is displayed.
		Does not ignore other errors. For development only-->
	<IngnoreEC1ForTesting>N</IngnoreEC1ForTesting>
  </NibpDal>
  <NIBP>
	<Repeat>
		<Rest>5</Rest>
	</Repeat>
	<AutoPWA>
		<Rest>5</Rest>
	</AutoPWA>
  </NIBP>
   <MyTestValues>
		<Value1>1</Value1>
		<Value2>2</Value2>
   </MyTestValues>
   <log4net>
		<appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender" >
			<param name="File" value="./system/Logs/scor.log" />
			<param name="AppendToFile" value="true" />
			<rollingStyle value="Composite" />
			<maxSizeRollBackups value="-1" />
			<maximumFileSize value="70KB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">			
        <param name="ConversionPattern" value="%d{MM/dd/yyyy} ~ %d{HH:mm:ss.fff} ~ [%c] - %m%n%n" />
			</layout>
		</appender>
		<root>
			<priority value="ALL" />
			<appender-ref ref="LogFileAppender" />
		</root>
		<category name="Scor">
			<priority value="ALL" />
		</category>
	</log4net>
</Parameters>