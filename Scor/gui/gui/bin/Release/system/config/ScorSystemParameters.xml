<?xml version="1.0" encoding="utf-8" ?>
<Parameters> 
   <TonometerPulseHeightVariationHigh>20</TonometerPulseHeightVariationHigh> 
   <TonometerPulseHeightVariationLow>10</TonometerPulseHeightVariationLow> 
   <CuffSignalQThreshold>70</CuffSignalQThreshold> 
   <NIBPRestInterval>5</NIBPRestInterval> 
   <StdEM4Timeout>50</StdEM4Timeout> 
   <ExtEM4Timeout>100</ExtEM4Timeout> 
   <EM4NumberOfRetriesAllowed>3</EM4NumberOfRetriesAllowed> 
   <ProcessSequenceNumber>
		<CommandResponse>N</CommandResponse>
		<StreamingPackets>N</StreamingPackets>
   </ProcessSequenceNumber>
   <IdleModeSleepTime>3000</IdleModeSleepTime>
   <ProcessStatusFlagParts>
		<StopButton>N</StopButton>
		<PowerUpBit>N</PowerUpBit>
		<AlarmFlag>N</AlarmFlag>
		<CuffStatusFlags>Y</CuffStatusFlags>
		<TonometerFlags>N</TonometerFlags>
		<UnusedBits>N</UnusedBits>
   </ProcessStatusFlagParts>
  <Gui>
    <WaitButtonDelay>5</WaitButtonDelay>
    <AutoPWAProgressbarDelay>15</AutoPWAProgressbarDelay>
  </Gui>
	<Crx>
		<Logging>
			<Enable>N</Enable>
	   </Logging>
	</Crx>
  <PWV>
    <Cuff>
      <Inflation>
        <Timeout>15</Timeout>
      </Inflation>
      <Deflation>
        <Timeout>10</Timeout>
      </Deflation>
      <Reinflation>
        <Rest>0</Rest>
      </Reinflation>
    </Cuff>
  </PWV>
  <PWA>
    <Cuff>
      <Inflation>
        <Timeout>10</Timeout>
      </Inflation>
	   <Deflation>
        <Timeout>10</Timeout>
      </Deflation>
      <Reinflation>
        <Rest>0</Rest>
      </Reinflation>
    </Cuff>
  </PWA>
  <RTOF>
 		<PWVTonometer>
			<MinNumberOfPulsesForAutoCapture>10</MinNumberOfPulsesForAutoCapture>
			<VariationUpperFeedbackLimePoint>70</VariationUpperFeedbackLimePoint>
			<VariationUpperFeedbackYellowPoint>85</VariationUpperFeedbackYellowPoint>
			<VariationLowerFeedbackLimePoint>150</VariationLowerFeedbackLimePoint>
			<VariationLowerFeedbackYellowPoint>160</VariationLowerFeedbackYellowPoint>
			<PHFeedbackLimePoint>190</PHFeedbackLimePoint>
			<PHFeedbackYellowPoint>220</PHFeedbackYellowPoint>
 		</PWVTonometer>
 		<PWVCuff>
			<MinNumberOfPulsesForAutoCapture>10</MinNumberOfPulsesForAutoCapture>
			<VariationUpperFeedbackLimePoint>68</VariationUpperFeedbackLimePoint>
			<VariationUpperFeedbackYellowPoint>75</VariationUpperFeedbackYellowPoint>
			<VariationLowerFeedbackLimePoint>8</VariationLowerFeedbackLimePoint>
			<VariationLowerFeedbackYellowPoint>12</VariationLowerFeedbackYellowPoint>
			<PHFeedbackLimePoint>55</PHFeedbackLimePoint>
			<PHFeedbackYellowPoint>60</PHFeedbackYellowPoint>
 		</PWVCuff>
 		<PWATonometer>
 			<VariationFeedbackLimePoint>5</VariationFeedbackLimePoint>
 			<PHFeedbackYellowPoint>75</PHFeedbackYellowPoint>
 		</PWATonometer>
 		<PWACuff>		
			<MinNumberOfPulsesForAutoCapture>10</MinNumberOfPulsesForAutoCapture>
			<VariationUpperFeedbackLimePoint>65</VariationUpperFeedbackLimePoint>
			<VariationUpperFeedbackYellowPoint>80</VariationUpperFeedbackYellowPoint>
			<VariationLowerFeedbackLimePoint>12</VariationLowerFeedbackLimePoint>
			<VariationLowerFeedbackYellowPoint>17</VariationLowerFeedbackYellowPoint>
			<PHFeedbackLimePoint>190</PHFeedbackLimePoint>
			<PHFeedbackYellowPoint>200</PHFeedbackYellowPoint>
 		</PWACuff>
 	</RTOF>
  <NibpCuffDataInterval>200</NibpCuffDataInterval>
   <MyTestValues>
		<Value1>1</Value1>
		<Value2>2</Value2>
   </MyTestValues>
   <log4net>
		<appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender" >
			<param name="File" value="./system/Logs/scor.log" />
			<param name="AppendToFile" value="true" />
			<rollingStyle value="Composite" />
			<maxSizeRollBackups value="-1" />
			<maximumFileSize value="70KB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">			
        <param name="ConversionPattern" value="%d{MM/dd/yyyy} ~ %d{HH:mm:ss.fff} ~ [%c] - %m%n%n" />
			</layout>
		</appender>
		<root>
			<priority value="ALL" />
			<appender-ref ref="LogFileAppender" />
		</root>
		<category name="Scor">
			<priority value="ALL" />
		</category>
	</log4net>
</Parameters>