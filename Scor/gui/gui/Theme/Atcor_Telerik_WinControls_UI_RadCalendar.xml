<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="Atcor" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration>
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="NormalButtonText,SegoeUIDefaultText">
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.RootRadElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonNormalFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonMouseOverFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseOver">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonMouseDownFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseDown">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonNormalBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonMouseOverBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseOver">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonMouseDownBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseDown">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="WhiteBackground,WhiteBorder">
            <Selectors>
              <XmlClassSelector ElementClass="RadCalendarElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="SkyBlueBorder,SkyBlueFill(DefaultVisible)">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.BorderGradientStyle" Value="Solid" />
            </PropertySettings>
            <Selectors>
              <XmlClassSelector ElementClass="CalendarNavigationElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellNormalFill,CalCellNormalBorder,NormalButtonText">
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellTodayFill,CalCellTodayBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.Today" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellSpecialDayFill,CalCellSpecialDayBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.SpecialDay" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellSelectedBorder,CalCellSpecialDayFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.Selected" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellMouseOverFill,CalCellMouseOverBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.MouseOver" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="WhiteBackground">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawBorder" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlClassSelector ElementClass="CalendarTableElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellNormalFill,CalCellNormalBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="ColumnHeaderVisualElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellMouseOverFill,CalCellSelectedBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="ColumnHeaderVisualElement.MouseOver" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellMouseOverFill,CalCellMouseOverBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RowHeaderVisualElement.MouseOver" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellOtherMonthText,CalCellNormalFill,CalCellNormalBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.OtherMonth" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellSpecialDayFill,CalCellSelectedBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.Focused" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="WhiteBackground(VisibleDefault)">
            <Selectors>
              <XmlClassSelector ElementClass="CalendarStatusElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CalCellNormalFill,CalCellNormalBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RowHeaderVisualElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="NormalButtonText">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawFill" Value="False" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawBorder" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.Header" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawFill" Value="False" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawBorder" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.Header.Selected" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawFill" Value="False" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawBorder" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="CalendarCellElement.Header.MouseOver" />
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadCalendar" ElementType="Telerik.WinControls.RootRadElement" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.RadCalendarElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>